<!-- ..................................  main page content starts ..............................  -->
<main>
    <!-- .....................  breadcrumbs starts ........................... -->
    <div class="breadcrumbBox">
        <div class="container">
            <div class="row">
                <div class="col-sm-12">
                    <ol class="breadcrumb">
                        <li><a href="#">Inicio</a></li>
                        <li class="active">Mis datos</li>
                    </ol>
                </div>
            </div>
        </div>
    </div>
    <!-- ....................  search result heading .......................... -->
    <div class="page-heading">
        <div class="container">
            <div class="row">
                <div class="col-sm-12">
                    <h5 class="text-center sub-heading">Listas de búsquedas más populares:</h5>

                    <h1 class="text-center page-heading">Búsquedas populares</h1>
                </div>
            </div>
        </div>
    </div>
    <!-- .....................  product listing starts ........................ -->
    <div class="product-listing">
        <div class="container">

            <!-- ................  popular product listing .....................  -->
          {{#each model.popularProductList as |obj|}}
              <div class="row">
                  <div class="col-sm-12">
                      <h3 class="page-block-heading">{{obj.alphabet}}:</h3>
                  </div>
              </div>
              <div class="row">
                  <div class="col-sm-12">
                      <div class="searchResult-popular">
                          <ul class="row">
                            {{#each obj.data as |productData|}}
                                <li class="col-sm-3"> {{#link-to 'home.search'  productData.storeType productData.productId productData.searchedText }} {{productData.searchedText}} {{/link-to}}</li>
                            {{/each}}
                          </ul>
                      </div>
                  </div>
              </div>
          {{else}}
              <li class="col-sm-3">No se han encontrado productos</li>
          {{/each}}

        </div>
    </div>
</main>
<!-- .............................  site footer starts ...................... -->
{{landing-page-popular-search-block data=model.popularSearch}}
